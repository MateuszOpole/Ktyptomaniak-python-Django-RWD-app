	<script type="text/javascript" >
var numer = 1;
			
			var timer1 = 0;
					var timer2 = 0;
			
			var tekst="";
			var idb=0;
			function zmienslajd(num, numtest)
			
			{ 
				if(numtest==1){
			if(num<idb||num>idb){numer=1;tekst="";idb=0; timer1=0;timer2=0;}
			if(num==3){tekst="wilk";idb=3;}
			if(num==2){tekst="sny";idb=2;}
			if(num==1){tekst="ben";idb=1;}
			
			
				
				var plik = "<img  class=\"picture\" src=\"img/"+tekst + numer + ".jpg\" / >";
				
			document.getElementById("pictures"+idb).innerHTML = plik;
			
				
				$("pictures"+idb).fadeIn(400);
					numer++; if (numer>3){ idb=0;
				tekst="";
				timer1=0;
				timer2=0;
				numer=1;
				return;}
				timer1 = setTimeout("zmienslajd(idb)", 5000);
				timer2 = setTimeout("schowaj(idb)", 4500);
			
				
				}
				if(numtest==2)
				{
				if(num==1){tekst="ben";idb=1;}
			if(num==2){tekst="sny";idb=2;}
			if(num==3){tekst="wilk";idb=3;}
				numer=3;
				
				var plik = "<img  class=\"picture\" src=\"img/"+ tekst+ numer + ".jpg\" / >";
				document.getElementById("pictures"+idb).innerHTML = plik;
				idb=0;
				tekst="";
				timer1=0;
				timer2=0;
				numer=1;
				return;
				
				
				}
				}
				function schowaj(num)
			{
				$("pictures"+num).fadeOut(500);
				}
				function stop(num)
				{
				if(num==1){tekst="ben";idb=1;}
			if(num==2){tekst="sny";idb=2;}
			if(num==3){tekst="wilk";idb=3;}
				numer=1;
				
				var plik = "<img  class=\"picture\" src=\"img/"+ tekst+ numer + ".jpg\" / >";
				document.getElementById("pictures"+idb).innerHTML = plik;
				idb=0;
				tekst="";
				timer1=0;
				timer2=0;
				numer=1;
				return;
				}
</script>
	
